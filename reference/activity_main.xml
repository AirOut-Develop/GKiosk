<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:focusable="true"
    android:focusableInTouchMode="true"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#000"
        android:gravity="center">

        <ImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/logo"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/wrap_StoreInfo"
        android:orientation="vertical"
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:padding="20dp"
        android:layout_gravity="top">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:layout_gravity="center">

            <LinearLayout
                android:id="@+id/store_info"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="지점 정보"
                    android:textSize="16sp"
                    android:textStyle="bold"/>
                <TextView
                    android:id="@+id/texView_StoreInfo"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="사업자 번호"
                    android:textSize="16sp"
                    android:textStyle="bold"/>

                <EditText
                    android:id="@+id/edit_companyNumber"
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    android:text=""
                    />

                <Button
                    android:id="@+id/button_get_store"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="불러오기"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#ddd"
            android:layout_marginVertical="25dp"/>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:layout_gravity="center">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="자판기 연결 정보"
                android:textSize="24sp"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:layout_gravity="center">

            <TextView
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="연결 정보"
                android:textSize="16sp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/textView_ven_isConnect"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="Disconnect"
                android:textSize="20sp"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="포트 번호"
                android:textSize="16sp"
                android:textStyle="bold"/>
            <Spinner
                android:id="@+id/spinner_ven_ports"
                android:layout_width="350dp"
                android:layout_height="wrap_content" />

            <Button
                android:id="@+id/button_ven_port_apply"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="선택 포트 적용"/>


        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="start"
            android:layout_gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="요청 전문"
                android:textSize="16sp"
                android:textStyle="bold"/>


            <EditText
                android:id="@+id/edit_ven_requestData"
                android:text=""
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:imeOptions="actionSend"
                android:inputType="numberDecimal"
                android:digits="0123456789"
                />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="start"
            android:layout_gravity="center">
            <TextView
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="응답 전문"
                android:textSize="16sp"
                android:textStyle="bold"/>


            <EditText
                android:id="@+id/edit_ven_resultData"
                android:text=""
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:imeOptions="actionSend"
                android:inputType="numberDecimal"
                android:digits="0123456789"
                />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="start"
            android:layout_gravity="center">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">


                <Button
                    android:id="@+id/button_vendNumberSetting"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="자판기 세팅"
                    android:visibility="visible"/>

                <Spinner
                    android:id="@+id/spinner_vendNumber"
                    android:layout_width="250dp"
                    android:layout_height="40dp"
                    android:entries="@array/VendNumber">
                </Spinner>

                <Button
                    android:id="@+id/button_vendOpen_all"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="전체 열기"/>


            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <EditText
                    android:id="@+id/edit_vendNumber"
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    />

                <Button
                    android:id="@+id/button_vendOpen_each"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="열기"/>

            </LinearLayout>

        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#ddd"
            android:layout_marginVertical="25dp"/>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:layout_gravity="center">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="카드 단말기 결제 테스트"
                android:textSize="24sp"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="start"
            android:layout_gravity="center">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="결제 금액"
                    android:textSize="16sp"
                    android:textStyle="bold"/>

                <EditText
                    android:id="@+id/edit_card_testPrice"
                    android:text="10"
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    />

                <Button
                    android:id="@+id/button_testPayment"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="결제 테스트"
                    android:onClick="onApprovalButtonClick"/>


            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:gravity="start"
            android:layout_gravity="center">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="요청 전문"
                    android:textSize="16sp"
                    android:textStyle="bold"/>


                <EditText
                    android:id="@+id/edit_card_requestData"
                    android:text=""
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:gravity="start"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="응답 전문"
                    android:textSize="16sp"
                    android:textStyle="bold"/>


                <EditText
                    android:id="@+id/edit_card_resultData"
                    android:text=""
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="start"
            android:layout_gravity="center">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">


                <TextView
                    android:layout_width="130dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="거래일련번호(20)"
                    android:textSize="16sp"
                    android:textStyle="bold"/>
                <EditText
                    android:id="@+id/edit_card_transactionSerial"
                    android:text=""
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:hint="NS240908155320167978"
                    />
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="승인번호(8)"
                    android:textSize="16sp"
                    android:textStyle="bold"/>
                <EditText
                    android:id="@+id/edit_card_approvalNumber"
                    android:text=""
                    android:layout_width="120dp"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    android:hint="00216525"
                    />
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="승인일자(6)"
                    android:textSize="16sp"
                    android:textStyle="bold"/>
                <EditText
                    android:id="@+id/edit_card_approvalDate"
                    android:text=""
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    android:hint="240101"
                    />

                <Button
                    android:id="@+id/button_card_cancelPayment"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="결제 취소"
                    android:onClick="onCancelButtonClick"/>

            </LinearLayout>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#ddd"
            android:layout_marginVertical="25dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:layout_gravity="center">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="지폐 투입기 연결 정보"
                android:textSize="24sp"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:layout_gravity="center">
            <!-- activity_admin.xml -->

            <TextView
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="연결 정보"
                android:textSize="16sp"
                android:textStyle="bold"/>
            <TextView
                android:id="@+id/textView_bill_isConnect"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="Disconnect"
                android:textSize="20sp"
                android:textStyle="bold"/>


            <TextView
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="포트 번호"
                android:textSize="16sp"
                android:textStyle="bold"/>

            <Spinner
                android:id="@+id/spinner_bill_ports"
                android:layout_width="300dp"
                android:layout_height="wrap_content" />
            <Button
                android:id="@+id/button_bill_port_apply"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="선택 포트 적용"/>


        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:gravity="start"
            android:layout_gravity="center">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <Button
                    android:id="@+id/button_bill_init"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="투입 시작"/>


                <Button
                    android:id="@+id/button_bill_check"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="상태 확인"/>

                <Button
                    android:id="@+id/button_bill_stop"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="투입 멈춤"/>

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="3.5"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="투입 필요 금액"
                    android:textSize="16sp"
                    android:textStyle="bold"/>

                <EditText
                    android:id="@+id/edit_bill_price"
                    android:text="3000"
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:imeOptions="actionSend"
                    android:inputType="numberDecimal"
                    android:digits="0123456789"
                    />


                <Button
                    android:id="@+id/button_bill_set"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="현금 투입 설정"/>

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="투입 금액"
                    android:textSize="16sp"
                    android:textStyle="bold"/>
                <TextView
                    android:id="@+id/textView_bill_input"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="출금 금액"
                    android:textSize="16sp"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/textView_bill_output"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textStyle="bold"/>


                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="합계 금액"
                    android:textSize="16sp"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/textView_bill_sum"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textStyle="bold"/>

                <Button
                    android:id="@+id/button_bill_reset"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="투입금 초기화"/>

            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:gravity="start"
                android:layout_gravity="center">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="start"
                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:text="요청 전문"
                        android:textSize="16sp"
                        android:textStyle="bold"/>


                    <EditText
                        android:id="@+id/edit_bill_requestData"
                        android:text=""
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:imeOptions="actionSend"
                        android:inputType="numberDecimal"
                        android:digits="0123456789"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:gravity="start"
                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:gravity="center"
                        android:text="응답 전문"
                        android:textSize="16sp"
                        android:textStyle="bold"/>


                    <EditText
                        android:id="@+id/edit_bill_resultData"
                        android:text=""
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:imeOptions="actionSend"
                        android:inputType="numberDecimal"
                        android:digits="0123456789"
                        />
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#ddd"
            android:layout_marginVertical="10dp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:layout_gravity="center">
            <TextView
                android:layout_width="300dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="WSS Data"
                android:textSize="14sp"
                android:textStyle="bold"/>

            <EditText
                android:id="@+id/wss_receive_msg"
                android:text=""
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:imeOptions="actionSend"
                android:inputType="numberDecimal"
                android:digits="0123456789"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <Button
                    android:id="@+id/wss_message_01"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="CONNECT TEST"/>

                <Button
                    android:id="@+id/wss_message_02"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="BUY TEST"/>


            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="개발자 모드"
                    android:textSize="16sp"/>
                <ToggleButton
                    android:id="@+id/devSettingToggleButton_MASTER"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:textOn="ON"
                    android:textOff="OFF"/>

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="start"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:text="테스트 결제"
                    android:textSize="16sp"/>
                <ToggleButton
                    android:id="@+id/devSettingToggleButton_Payment"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:textOn="ON"
                    android:textOff="OFF"/>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>



    <LinearLayout
        android:id="@+id/wrap_enable_disable"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:layout_gravity="top">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="5dp"
            android:background="#ddd"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="키오스크 모드"
            android:textColor="#333"
            android:textSize="13sp"
            android:textAlignment="center"
            android:layout_marginBottom="10dp"/>
        <Button
            android:id="@+id/button_KioskModeControl"
